<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.day2.en.edu.scau.dao.TmpDao">

<insert id="insert" parameterType="Tmp">
	insert into tmp values (#{number}, #{title})
</insert>
<select id="findAll" resultType="Tmp">
	select * from tmp;
</select>
</mapper>