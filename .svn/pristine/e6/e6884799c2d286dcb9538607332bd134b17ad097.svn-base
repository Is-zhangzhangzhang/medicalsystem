// 点击登录按钮效果
$('#login-button').click(function(event){
	event.preventDefault();
	$('.lo-form').fadeOut(500);
	$('.wrapper').addClass('form-success');
	setTimeout(function () {
        window.location.href = "./html/main.html";
    },1000);
});

$('#to-register').click(function(event){
	$('.lo-form').fadeOut(100);
	$('.re-form').fadeIn();
});

$('#re-born').datetimepicker({
	format:'yyyy-mm-dd',
	minView: "month",
	autoclose:1
})


